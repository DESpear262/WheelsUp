# WheelsUp Web Application Environment Variables
# Copy this file to .env and fill in your actual values

# Database Configuration
DATABASE_URL=postgresql://wheelsup_user:wheelsup_password@postgres:5432/wheelsup_dev?schema=public

# Search Configuration
OPENSEARCH_URL=http://opensearch:9200

# Mapbox Configuration (for mapping features)
NEXT_PUBLIC_MAPBOX_TOKEN=your_mapbox_token_here

# Application URLs
NEXT_PUBLIC_API_URL=http://localhost:3000
NEXT_PUBLIC_APP_URL=http://localhost:3000

# Development Configuration
NODE_ENV=development
NEXT_TELEMETRY_DISABLED=1

# Authentication (if needed for admin features)
# AUTH_SECRET=your_auth_secret_here
# AUTH_URL=http://localhost:3000/api/auth

# Logging and Monitoring
# SENTRY_DSN=your_sentry_dsn_here

# Feature Flags
NEXT_PUBLIC_ENABLE_DEBUG=true
NEXT_PUBLIC_ENABLE_ANALYTICS=false

# External APIs (for future use)
# AIRPORT_API_KEY=your_airport_api_key_here
# WEATHER_API_KEY=your_weather_api_key_here
