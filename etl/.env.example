# WheelsUp ETL Pipeline Environment Variables
# Copy this file to .env and fill in your actual values

# Database Configuration
DATABASE_URL=postgresql://wheelsup_user:wheelsup_password@postgres:5432/wheelsup_dev?schema=public

# Search Configuration
OPENSEARCH_URL=http://opensearch:9200

# AWS Configuration (for S3, Bedrock, etc.)
AWS_ACCESS_KEY_ID=your_aws_access_key_here
AWS_SECRET_ACCESS_KEY=your_aws_secret_access_key_here
AWS_DEFAULT_REGION=us-east-1
AWS_REGION=us-east-1

# LLM API Keys
ANTHROPIC_API_KEY=your_anthropic_api_key_here
OPENAI_API_KEY=your_openai_api_key_here

# ETL Configuration
ETL_SNAPSHOT_ID=dev-test-001
ETL_BATCH_SIZE=10
ETL_MAX_RETRIES=3
ETL_TIMEOUT_SECONDS=300

# Data Sources
ENABLE_FAA_SCRAPING=true
ENABLE_SCHOOL_WEBSITE_SCRAPING=true
ENABLE_GOOGLE_PLACES_API=false
ENABLE_REDDIT_SCRAPING=false

# Logging and Monitoring
LOG_LEVEL=INFO
SENTRY_DSN=your_sentry_dsn_here

# Output Configuration
S3_BUCKET_RAW=your-raw-data-bucket
S3_BUCKET_PARSED=your-parsed-data-bucket
S3_BUCKET_PUBLISHED=your-published-data-bucket

# Development Configuration
PYTHONPATH=/app
DEBUG=true
